<%= simple_form_for [type, promo] do |f| %>
  <%= f.input :title %>
  <%= f.input :description %>
  <%= f.input :start, as: :date, start_year: Date.today.year, discard_day: true, order: [:month, :year] %>
  <%= f.input :end, as: :date, start_year: Date.today.year, discard_day: true, order: [:month, :year] %>
  <%= f.input :current %>
  <%= f.input :photos, as: :attachinary %>
  <%= f.submit %>
<% end %>
